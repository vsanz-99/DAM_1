package colecciones_diccionarios;
import java.util.ArrayList;

/**
 * <p>Crear un ArrayList con los nombres de 6 
 * compannieros de clase. A continuación, mostrar los 
 * nombre por pantalla. Utilizar un bucle for que recorra el ArrayList 
 * sin usar ningun indice.</p>
 * @since 14/05/2022
 * @version 1.0
 * @author Victor Sanz*/

public class S10Ejercicio01 {

	public static void main(String[] args) {
		 
		ArrayList<String> a=new ArrayList<String>();
		
		a.add("Jose Manuel");
		a.add("Salvador");
		a.add("Ruben");
		a.add("Irene");
		a.add("Noemi");
		a.add("Begonnia");
		
		System.out.println("\tContenido de las listas: ");
		
		for (String nombre : a) {
			System.out.println("\t"+nombre);
		}

	}

}
